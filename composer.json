{
    "name": "mikk150/yii2-phone-validator",
    "description": "yii2 phonenumber validator",
    "type": "yii2-extension",
    "keywords": ["yii2","validator","libphonenumber"],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Mikk Tendermann",
            "email": "mikk150@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0",
        "bower-asset/google-libphonenumber": "~3.0|~2.0",
        "giggsey/libphonenumber-for-php": "~8.3"
    },
    "require-dev": {
        "codeception/codeception": "^3.0 | ^4.0 | ^5.0",
        "codeception/module-asserts": "^1.0 | ^2.0 | ^3.0",
        "credy/phpbuiltinserver": "dev-master"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "mikk150\\phonevalidator\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
